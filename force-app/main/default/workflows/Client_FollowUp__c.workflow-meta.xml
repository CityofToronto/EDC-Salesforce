    <rules>
        <fullName>Client Follow-Up Survey - 30%2F60%2F90 Days Company intelligence</fullName>
        <actions>
            <name>InitialSurveyStageUpdate</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <description>This workflow rule will trigger Business Outcome Survey after 30/60/90 days for Company Intelligence Interaction</description>
        <formula>AND(TEXT(Workflow_Session__c)=&#39;Company Intelligence Interaction&#39;, NOT(Contact__r.HasOptedOutOfEmail),  NOT(Initial_Survey_Completed__c), NOT(ISBLANK(Survey_Start_Date__c)))</formula>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
        <workflowTimeTriggers>
            <actions>
                <name>Client_Follow_Up_Survey_Email_30_Day</name>
                <type>Alert</type>
            </actions>
            <actions>
                <name>X30DaysSurveyStageUpdate</name>
                <type>FieldUpdate</type>
            </actions>
            <offsetFromField>Client_FollowUp__c.Survey_Start_Date__c</offsetFromField>
            <timeLength>30</timeLength>
            <workflowTimeTriggerUnit>Days</workflowTimeTriggerUnit>
        </workflowTimeTriggers>
