<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Validation_Interaction_Issue_Account</fullName>
    <active>true</active>
    <errorConditionFormula>AND(NOT(ISBLANK(Account__c)),NOT(ISBLANK(Interaction__c)),NOT(ISBLANK(Interaction__r.Account__c)), Account__c &lt;&gt; Interaction__r.Account__c)</errorConditionFormula>
    <errorMessage>Interaction  Account and Issue Account are not same.</errorMessage>
</ValidationRule>
