<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Validation_Agreement_Issue_Account</fullName>
    <active>true</active>
    <errorConditionFormula>AND(NOT(ISBLANK(Agreement__c)),NOT(ISBLANK(Agreement__r.Account_Name__c)),NOT(ISBLANK(Account__c)), Agreement__r.Account_Name__c &lt;&gt; Account__c)</errorConditionFormula>
    <errorMessage>Agreement Account and Issue Account are not same.</errorMessage>
</ValidationRule>
