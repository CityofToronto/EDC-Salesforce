<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Validation_to_Lock_Starter_Company_Grant</fullName>
    <active>true</active>
    <description>This rule is to validate Starter Company Grant application so that it can not be changed submitted through online application form.</description>
    <errorConditionFormula>AND($User.ProfileId&lt;&gt;&#39;00eo0000000wf4I&#39;,RecordType.DeveloperName=&#39;Starter_Company_Grant&#39;,CreatedBy.Id=&#39;0051N0000078NiC&#39;,OR(ISCHANGED(Account__c),ISCHANGED(Contact__c),ISCHANGED(BirthDate__c),ISCHANGED(Gender__c),ISCHANGED(Visible_Minority__c),ISCHANGED(Legal_Structure__c),ISCHANGED(Describe_your_credit_rating__c),ISCHANGED(Sector__c),ISCHANGED(Proposing_a_new_or_existing_business__c),ISCHANGED(Are_you_18_and_over__c),ISCHANGED(A_Canadian_citizen_or_permanent_resident__c),ISCHANGED(Received_a_Starter_Company_grant_before__c),ISCHANGED(Received_another_grant_before__c),ISCHANGED(If_yes_please_identify__c),ISCHANGED(Do_you_have_a_location_in_Toronto__c),ISCHANGED(Commit_to_the_training_program__c),ISCHANGED(Demonstrate_25_commitment__c),ISCHANGED(CurrentExisting_Retail_Business__c),ISCHANGED(Proposed_Pricing_to_Target_Customers__c),ISCHANGED(Marketing_Plan__c),ISCHANGED(Total_Monthly_Expense__c),ISCHANGED(Total_Monthly_Revenue__c),ISCHANGED(Breakdown_of_how_you_spend_the_Grant__c),ISCHANGED(Monthly_Expense_Details__c),ISCHANGED(Application_Stream__c)))</errorConditionFormula>
    <errorMessage>Field Value for Account, Contact or any field in Application Contact Information, Business Information, Eligibility Criteria, Eligibility Criteria and Financials can not be changed for application submitted through Online Form.</errorMessage>
</ValidationRule>
